{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
          "waf0Prefix": {
               "type": "string",
               "minLength": "1",
               "metadata": {
                    "description": "Prefix of the first WAF"
               }
          }
	},
	"variables": {
	},
	"resources": [{
		"type": "Microsoft.Compute/virtualMachines/extensions",
		"name": "[concat(parameters('waf0Prefix'),0,'/iAppDeployment')]",
		"location": "[resourceGroup().location]",
		"apiVersion": "2015-06-15",
		"tags": {
			"displayName": "iAppDeployment"
		},
		"properties": {
             "publisher": "Microsoft.OSTCExtensions",
             "type": "CustomScriptForLinux",
             "typeHandlerVersion": "1.2",
             "settings": {
               "fileUris": [
                 "http://cdn.f5.com/product/blackbox/staging/azure/deploy_iapp.sh"
                ]
			},
			"commandToExecute": " sh deploy_iapp.sh"
		}
	}],
	"outputs": {}
}